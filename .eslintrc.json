{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "no-use-before-define": 0,
    "quotes": ["worn", "single"],
    "react/prop-types": ["error", { "ignore": ["navigation"] }]
  }
}
